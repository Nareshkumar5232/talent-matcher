{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/index.js": {
            "includeFiles": "backend/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        }
    ]
}